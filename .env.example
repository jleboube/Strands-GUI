# Database Configuration
DB_NAME=strands_gui
DB_USER=strands
DB_PASSWORD=your_secure_password_here

# JWT Secret (generate with: openssl rand -hex 32)
JWT_SECRET=your_jwt_secret_here_change_in_production

# Domain Configuration (optional)
# Set this if you're using a custom domain (e.g., with Cloudflare Tunnels)
# Leave empty or commented out for localhost access
DOMAIN_NAME=
# Set to true if your domain uses HTTPS (recommended for production)
USE_HTTPS=false

# CORS Origins (comma-separated)
# When DOMAIN_NAME is set, it will be automatically added to CORS
CORS_ORIGINS=http://localhost:58431,http://127.0.0.1:58431

# Debug Mode
DEBUG=false

# AWS Credentials (for Bedrock)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# Optional: OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Optional: Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Optional: Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key
